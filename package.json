{
	"name": "create-electron-react-app",
	"description": "Use React with Electron easily.",
	"author": "Levminer",
	"version": "1.1.0",
	"license": "MIT",
	"scripts": {
		"dev": "npm run pack && npm run start",
		"watch": "concurrently 'npm:watcher' 'npm:electronmon'",
		"watcher": "webpack --config .webpackrc.js --watch",
		"pack": "webpack --config .webpackrc.js",
		"start": "electron .",
		"electronmon": "electronmon .",
		"build": "electron-builder",
		"rebuild": "electron-rebuild"
	},
	"build": {
		"productName": "create-electron-react-app",
		"appId": "com.levminer.create-electron-react-app",
		"win": {
			"target": [
				"nsis"
			]
		}
	},
	"electronmon": {
		"patterns": [
			"!renderer/*",
			"!dist/*"
		]
	},
	"main": "./main/main.js",
	"dependencies": {
		"@ant-design/icons": "^4.4.0",
		"antd": "^4.20.6",
		"autosize": "^5.0.1",
		"axios": "^0.27.2",
		"babel-plugin-macros": "^3.0.1",
		"beautiful-react-hooks": "^1.0.2",
		"chokidar": "^3.5.3",
		"concurrently": "^7.2.2",
		"electron-devtools-installer": "^3.1.1",
		"electron-reload": "^1.5.0",
		"ext-name": "^5.0.0",
		"isbinaryfile": "^5.0.0",
		"lodash": "^4.17.21",
		"lodash.clonedeep": "^4.5.0",
		"node-pty": "^0.9.0",
		"prismjs": "^1.23.0",
		"qs": "^6.10.5",
		"re-resizable": "^6.9.0",
		"react-contenteditable": "^3.3.6",
		"react-custom-scrollbars": "^4.2.1",
		"react-devtools-electron": "^4.7.0",
		"react-icons": "^4.4.0",
		"react-resizable": "^1.11.0",
		"react-resize-detector": "^7.0.0",
		"react-resize-panel": "^0.3.5",
		"simplebar-react": "^2.4.1",
		"styled-components": "^5.2.1",
		"use-debounce": "^8.0.1",
		"valid-filename": "^4.0.0",
		"xterm": "^4.16.0",
		"xterm-addon-fit": "^0.5.0",
		"xterm-addon-web-links": "^0.5.0",
		"xterm-for-react": "^1.0.4"
	},
	"devDependencies": {
		"@babel/core": "^7.14.8",
		"@babel/preset-env": "^7.14.8",
		"@babel/preset-react": "^7.14.5",
		"babel-loader": "^8.2.2",
		"concurrently": "^6.2.0",
		"css-loader": "^6.2.0",
		"electron": "^8.5.5",
		"electron-builder": "^22.10.5",
		"electronmon": "^2.0.1",
		"prettier": "^2.3.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^5.2.0",
		"sass": "^1.36.0",
		"sass-loader": "^12.1.0",
		"style-loader": "^3.2.1",
		"webpack": "^5.47.0",
		"webpack-cli": "^4.7.2"
	}
}
